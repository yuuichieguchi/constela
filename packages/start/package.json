{
  "name": "@constela/start",
  "version": "1.5.5",
  "description": "Meta-framework for Constela applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "constela-start": "./bin/constela-start.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/runtime/entry-client.d.ts",
      "import": "./dist/runtime/entry-client.js"
    },
    "./server": {
      "types": "./dist/runtime/entry-server.d.ts",
      "import": "./dist/runtime/entry-server.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts src/runtime/entry-client.ts src/runtime/entry-server.ts src/cli/index.ts --format esm --dts --clean",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@constela/compiler": "workspace:*",
    "@constela/core": "workspace:*",
    "@constela/router": "workspace:*",
    "@constela/runtime": "workspace:*",
    "@constela/server": "workspace:*",
    "commander": "^12.0.0",
    "esbuild": "^0.24.0",
    "fast-glob": "^3.3.0",
    "gray-matter": "^4.0.0",
    "hono": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.0",
    "vite": "^6.0.0",
    "postcss": "^8.5.0",
    "@tailwindcss/postcss": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "@types/mdast": "^4.0.4",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "license": "MIT"
}
